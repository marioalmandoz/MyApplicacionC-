<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MyApplicacion.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MyApplicacion"
    Title="MyApplicacion"
    Shell.FlyoutBehavior="Disabled">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
        Title="Produccion"
        ContentTemplate="{DataTemplate local:ProduccionPage}"
        Route="ProduccionPage" />
    <ShellContent
        Title="Almacen"
        ContentTemplate="{DataTemplate local:AlmacenPage}"
        Route="AlmacenPage" />
    <ShellContent
        Title="Ok"
        ContentTemplate="{DataTemplate local:OkPage}"
        Route="OkPage" />
    <ShellContent
        Title="Recepcion"
        ContentTemplate="{DataTemplate local:RecepcionPage}"
        Route="RecepcionPage" />
    <ShellContent
        Title="Ubicacion"
        ContentTemplate="{DataTemplate local:UbicacionPage}"
        Route="UbicacionPage" />
    <ShellContent
        Title="Pedido"
        ContentTemplate="{DataTemplate local:PedidoPage}"
        Route="PedidoPage" />


</Shell>
